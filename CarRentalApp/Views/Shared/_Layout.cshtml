<!DOCTYPE html>
<html lang="en" dir="rtl">
<head>
    <meta charset="utf-8" />
   
    <title>@ViewData["Title"] - Agency</title>

    <link href="~/lib/bootstrap/css/bootstrap.css" rel="stylesheet" />
    <link href="~/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/lib/bootstrap/css/bootstrap.rtl.css" rel="stylesheet" />
    <link href="~/lib/bootstrap/css/bootstrap.rtl.min.css" rel="stylesheet" />

    <link href="~/lib/bootstrap-icons/font/bootstrap-icons.min.css" rel="stylesheet" />
    <link href="~/lib/bootstrap-icons/]font/bootstrap-icons.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css2?family=Amiri&display=swap" rel="stylesheet">
    <link href="~/css/sidebar.css" rel="stylesheet" />
    <link href="~/lib/bootstrap-datepicker/css/bootstrap-datepicker.css" rel="stylesheet" />
    <link href="~/lib/bootstrap-datepicker/css/bootstrap-datepicker.min.css" rel="stylesheet" />
    <link href="~/lib/bootstrap-datepicker/css/bootstrap-datepicker.standalone.css" rel="stylesheet" />
    <link href="~/lib/bootstrap-datepicker/css/bootstrap-datepicker.standalone.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">
   
</head>
<body class="container-fluid">
    <!-- Navbar (fixed to top) -->
    <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-primary border-bottom box-shadow mb-3 text-white">
        <div class="container-fluid">
            <a class="navbar-brand text-white" asp-area="" asp-controller="Home" asp-action="Index">شركة كناز لتأجير السيارات</a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                    aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                <ul class="navbar-nav flex-grow-1">
                    <li class="nav-item">
                        <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="Index">الرئيسية</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="Privacy">حول المظام</a>
                    </li>
                </ul>
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <form action="/Account/Logout" method="post" style="display: inline;">
                            <button type="submit" class="btn btn-danger">خـروج</button>
                        </form>
                    </li>
                </ul>
            </div>
        </div>
    </nav>


    
    <!-- Sidebar (Fixed on the left) -->
    <div class="sidebar bg-primary" style="co">
        <div style="padding-top:50px"></div>
        <ul class="nav flex-column">
            <li class="nav-item">
                <a asp-controller="dashboard" asp-action="Index" class="nav-link">
                    <i class="fas fa-tachometer-alt"></i> لوحة التحكم
                </a>
            </li>
            <li class="nav-item">
                <a asp-controller="Branch" asp-action="Index" class="nav-link">
                    <i class="fas fa-cogs"></i> فروع الشركة
                </a>
            </li>
            <li class="nav-item">
                <a asp-controller="Account" asp-action="Index" class="nav-link">
                    <i class="fas fa-user"></i> المستخدمين
                </a>
            </li>
            <li class="nav-item">
                <a asp-controller="Company" asp-action="Index" class="nav-link">
                    <i class="fas fa-building"></i> شركات التأمين
                </a>
            </li>
            <li class="nav-item">
                <a asp-controller="Manufactorer" asp-action="Index" class="nav-link">
                   
                    <i class="fas fa-industry"></i> الشركات المصنعة
                </a>
            </li>
            <li class="nav-item">
                <a asp-controller="Modeel" asp-action="Index" class="nav-link">
                    <i class="fas fa-car"></i> طراز السيارات
                </a>
            </li>
            <li class="nav-item">
                <a asp-controller="Classification" asp-action="Index" class="nav-link">
                    <i class="fas fa-car-side"></i> تصنيف السيارات
                </a>
            </li>
            <li class="nav-item">
                <a asp-controller="Color" asp-action="Index" class="nav-link">
                    <i class="fas fa-color"></i> ألوان السيارات
                </a>
            </li>
        </ul>
    </div>

    <div class="container" style="padding-right:150px">
        <main role="main" class="pb-3">
            @RenderBody()

        </main>

    </div>



    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)


</body>
@* <footer class="border-top footer text-muted">
    <div class="container">
        &copy; 2024 - Agency - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
    </div>

</footer> *@
</html>
